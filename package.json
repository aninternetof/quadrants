{
  "name": "quadrants",
  "version": "0.0.1",
  "description": "Decision making via quadrants.",
  "main": "server.js",
  "scripts": {
    "watch": "watch 'npm run build-elm' source",
    "dev": "elm-live source/Main.elm --open --dir=public/ -- --debug --output=public/elm.js",
    "build-elm": "elm make source/Main.elm --output public/elm.js",
    "start": "npm run build-elm && node server.js"
  },
  "devDependencies": {
    "elm": "^0.19.1-3",
    "elm-live": "^4.0.2",
    "natives": "^1.1.6",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "express": "^4.16.3",
    "elm-upgrade": "0.19.2",
    "elm-format": "^0.8.0"
  },
  "engines": {
    "node": "8.x"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/welcome-project"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express",
    "elm"
  ]
}
